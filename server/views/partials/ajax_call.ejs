<script src="/javascripts/plotWinLoss.js"></script>
<script src="/javascripts/plotGoals.js"></script>
<script type="text/javascript">
  $(document).ready(function(){
    $.ajax({
      method: 'get',
      url: '/api/<%= team %>',
      dataType: 'json',
      success: function(data){
        plotWinLoss(data);
        plotGoals(data);
      }
    });
  });
</script>
