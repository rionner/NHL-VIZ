<script src="/javascripts/plotWinLoss.js"></script>
<script src="/javascripts/plotGoals.js"></script>
<script type="text/javascript">
  $(document).ready(function(){
    $.ajax({
      method: 'get',
      url: '/api/<%= team %>',
      dataType: 'json',
      success: function(data, colors){
        var years = [];
        var colors = data[data.length - 1].teamColors[0];
        for (var i = 0; i < data.length; i++) {
          if (data[i].regular_season) years.push(data[i].years);
        }
        plotWinLoss(data, colors, years);
        // plotGoals(data, years);
      }
    });
  });
</script>
